{
    "manifest_version": 3,
    "name": "Return 100 Results",
    "description": "Return 100 Google search results instead of 10",
    "version": "1.1",
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "*://*.google.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/hunnit16.png",
        "48": "icons/hunnit48.png",
        "128": "icons/hunnit128.png"
    }
}
